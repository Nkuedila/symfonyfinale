{% extends 'base.html.twig' %}

{% block title %}Hello MailerController!{% endblock %}

{% block body %}
<h1>New Commande</h1>

<p><strong>Commande ID</strong>{{ commande.id }}</p>
<p><strong>Client Name:</strong>{{ commande.nomClient }}</p>
<p><strong>Email:</strong>{{ commande.emailClient }}</p>
<p><strong>Quantity:</strong>{{ commande.quantity }}</p>
<p><strong>Total:</strong>{{ commande.total }}</p>
<p><strong>Address:</strong>{{ commande.adresseClient }}</p>





{% endblock %}
